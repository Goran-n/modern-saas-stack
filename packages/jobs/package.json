{
  "name": "@figgy/jobs",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "doppler run -p figgy-be -c dev -- npx trigger.dev@latest dev",
    "deploy": "doppler run -p figgy-be -c dev -- npx trigger.dev@latest deploy",
    "deploy:staging": "doppler run -p figgy-be -c staging -- npx trigger.dev@latest deploy --env staging",
    "deploy:prod": "doppler run -p figgy-be -c production -- npx trigger.dev@latest deploy --env prod",
    "typecheck": "tsc --noEmit",
    "extract": "bun src/lib/document-extraction/cli.ts"
  },
  "exports": {
    ".": "./src/index.ts",
    "./schemas": "./src/schemas/index.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.12",
    "@figgy/config": "workspace:*",
    "@figgy/deduplication": "workspace:*",
    "@figgy/shared-db": "workspace:*",
    "@figgy/supplier": "workspace:*",
    "@figgy/supabase-storage": "workspace:*",
    "@figgy/utils": "workspace:*",
    "@figgy/llm-utils": "workspace:*",
    "@trigger.dev/sdk": "^3.3.17",
    "ai": "^4.3.19",
    "commander": "^14.0.0",
    "unpdf": "^0.12.2"
  },
  "devDependencies": {
    "@trigger.dev/build": "^3.3.17",
    "trigger.dev": "^3.3.17",
    "tsx": "^4.20.3"
  }
}
