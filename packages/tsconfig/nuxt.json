{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./vue.json",
  "compilerOptions": {
    "jsx": "preserve",
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "allowJs": true,
    "incremental": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "types": ["@nuxt/types", "@nuxt/ui", "@types/node"]
  }
}
